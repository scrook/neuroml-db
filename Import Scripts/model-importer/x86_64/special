#!/bin/sh
if test "x${NRNHOME}" = "x" ; then
	NRNHOME="/home/justas/neuron/nrn"
fi
if test "x${NRNBIN}" = "x" ; then
	NRNBIN="/home/justas/neuron/nrn/x86_64/bin/"
fi
if test "" = "yes" ; then
	NRNIV="${NRNBIN}nrniv.app/Contents/MacOS/nrniv"
else
	NRNIV="${NRNBIN}nrniv"
fi
"${NRNIV}" -dll "/home/justas/Repositories/neuroml-db/Import Scripts/model-importer/x86_64/.libs/libnrnmech.so" "$@"
