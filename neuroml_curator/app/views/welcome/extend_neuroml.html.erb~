&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span><b>&lt;xs:group ref=<span style="color:blue">"meta:metadata"</span>/</a>&gt;</b></span><span style="color:green"><br/>
<br/>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span><b>&lt;xs:element name=<span style="color:blue">"cells"</span> type=<a href =#neuroml:Level3Cells><span style="color:blue">"neuroml:Level3Cells"</span></a> minOccurs=<span style="color:blue">"0"</span>/</a>&gt;</b></span><span style="color:green"><br/>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br/>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span><b>&lt;xs:element name=<span style="color:blue">"vasculature"</span></a>&gt;</b></span><span style="color:green"><br/>
