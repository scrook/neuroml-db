<div style="margin-top: 1em;" id="ont_results">
<% count=0 %>
<% shownModels = [] %>
<% for key,value in result_hash %>
   <%
   if key == "ModelNames" or value.to_a.length == 0
     next
   end
   %>

   <% for relationship in value %>
       <% temparray=relationship["ModelIds"].to_a %>

       <% for eachid in temparray %>
          <%
          if shownModels.include? ont_ids[count] then
            count=count + 1
            next
          else
            shownModels.push(ont_ids[count])
          end
          %>

          <div><a <%= model_atag_params(ont_ids[count]) %> title="Model Information for <%=ont_ids[count]%>" ><span style="font-weight:bold"><%= ont_names[count] %></span></a>
            <div style="color:gray; margin-left:20px;font-size:14px;">From: <%=Model.GetModelShortPub(ont_ids[count])%></div>
          </div>

          <% if ont_relations && ont_relations.length > 0 && ont_relations[count] %>
            <% if ont_relations[count][:Type] == :Gap %>

              <div style="padding-left: 20px; font-size: 14px;"><%= ont_relations[count][:End1][:Property] %> <span style="font-weight:bold"><%= ont_relations[count][:End1][:Subject] %></span></div>
              <div style="padding-left: 20px; font-size: 14px; margin-bottom: 15px;"><%= ont_relations[count][:End2][:Property] %> <span style="font-weight:bold"><%= ont_relations[count][:End2][:Subject] %></span></div>

            <% elsif ont_relations[count][:Type] == :Direct || ont_relations[count][:Type] == :Keyword %>

              <div style="padding-left: 20px; font-size: 14px; margin-bottom: 15px;"><%= ont_relations[count][:Property] %> <span style="font-weight:bold"><%= ont_relations[count][:Subject] %></span></div>

            <% end %>

          <% end %>


          <% count=count + 1 %>

       <% end %>

   <%  end %>
<% end %>
</div>
