                        <ul>

                            <li><a href="/browse_models"><span>Browse Models</span></a></li>
                            <!--									<li><a href="/model_submit"><span>Submit Models</span></a></li>
                                                               <li><a href="/search_result"><span>Search Models</span></a></li>
                                                               <li><a href="/add_cell"><span>Add Cell</span></a></li>
                            -->									
                        </ul> 
                    </li>
                    <li <% if community == 1 %>class="<%= active_class %>" <% end %>><a href="#">Community</a>
                        <ul>
                            <li><a href="/history"><span>History of NeuroML</span></a></li>
                            <li><a href="/editors"><span>NeuroML Editorial Board</span></a></li>
                            <li><a href="/scientific_committee"><span>Scientific Committee</span></a></li>
                            <li><a href="/acknowledgements"><span>Acknowledgements</span></a></li>

                        </ul>
                    </li>

                    <li <% if development == 1 %>class="<%= active_class %>" <% end %>><a href="#">Development</a>
                        <%#= render_menu :top_menu if User.current.logged? || !Setting.login_required? -%>
                        <ul>
                            <li><a href="/getneuroml"><span>Get NeuroML</span></a></li>
                        </ul>

                    </li>
                </ul>
                <!-- Navigation -->
            </div>
            <!-- wrapper-menu -->
        </div>
        <!-- ENDS menu-holder -->
    </div>
