                            <li><a href="http://lems.github.io/LEMS/"><span>LEMS</span></a></li>
                            <li><a href="/relevant_publications"><span>Publications</span></a></li>
                            <li><a href="/workshops"><span>Workshop Reports</span></a></li>
                        </ul>
                    </li>
                    <li <% if tools == 1 %>class="<%= active_class %>" <% end %>><a href="#">Tools</a>
                        <ul>

                            <li><a href="/tool_support"><span>Tool Support</span></a></li>
                            <li><a href="/validate"><span>NML Validator</span></a></li>
                            <li><a href="/neuron_tools"><span>Neuron Tools</span></a></li>
                            <li><a href="/pynn"><span>PyNN</span></a></li>
                            <li><a href="/x3dtools"><span>X3D Tools</span></a></li>
                        </ul>
                    </li>
                    <!--	<li ><a href="/browse_models">Models</a> -->

