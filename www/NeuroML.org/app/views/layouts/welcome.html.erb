                </a>
                <div id="caption-text">
                    <span>A Model Description Language for Computational Neuroscience</span>
                </div>

                

            </div>
            <!-- ENDS search -->
        </div>
        <!-- ENDS wrapper-header -->					
    </div>
    <!-- ENDS HEADER -->


    <!-- Menu -->
    <div id="menu">
        <%
        active_class="not_active"
        if controller.action_name == "index"
        index=1
        active_class="current-menu-item"
        elsif ['neuromlv2','extend_neuroml','lems','relevant_publications','workshops','compatibility','specifications','examples','lems_dev','mappings','libnml'].include? controller.action_name
        documents=1
        active_class="current-menu-item"
        elsif ['tool_support','validate','neuron_tools','pynn','x3dtools'].include? controller.action_name
        tools=1
        active_class="current-menu-item"
        elsif ['browse_models','search_result','model_submit'].include? controller.action_name
        models=1
        active_class="current-menu-item"
        elsif ['history','scientific_committee','acknowledgements'].include? controller.action_name
        community=1
        active_class="current-menu-item"
        elsif ['projects','guide','getneuroml'].include? controller.action_name
        development=1
        active_class="current-menu-item"
        end


        %>


        <!-- ENDS menu-holder -->
        <div id="menu-holder">
            <!-- wrapper-menu -->
            <div class="wrapper">
                <!-- Navigation -->
                <ul id="nav" class="sf-menu">
                    <li <% if index == 1 %>class="<%= active_class %>" <% end %> ><a href="/home">Home</a></li>
                    <li <% if documents == 1 %>class="<%= active_class %>" <% end %> ><a href="#">Documents</a>
                        <ul>
                            <li><a href="/specifications"><span>Specifications v1.8.1</span></a></li>
